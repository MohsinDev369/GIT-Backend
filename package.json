{
  "name": "geatit-next-backend",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run db:push && next start",
    "dev": "npm run db:push && next dev --turbopack",
    "build": "npm run db:push && next build",
    "lint": "next lint",
    "db:generate": "npx drizzle-kit generate",
    "db:push": "npx drizzle-kit push",
    "db:getddl": "npx drizzle-kit introspect",
    "venue-seed": "bun run ./src/scripts/confirm-and-run.ts bun run ./src/seed/seed-venues.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "canvas": "^3.1.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.42.0",
    "drizzle-seed": "^0.3.1",
    "jsdom": "^26.1.0",
    "next": "15.3.0",
    "next-swagger-doc": "^0.4.1",
    "pg": "^8.15.6",
    "postgres": "^3.4.5",
    "qr-code-styling-node": "^1.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
